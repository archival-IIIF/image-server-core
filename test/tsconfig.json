{
  "compilerOptions": {
    "strict": true,
    "target": "es2020",
    "lib": [
      "es2020"
    ],
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "outDir": "./dist",
    "declaration": true
  },
  "include": [
    "./**/*",
  ],
}
